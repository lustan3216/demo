<% if @booking.errors.any? %>
      <ul>
      <% @booking.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
<% end %>
</br>

<%=form_for @booking,:url => bookings_path do |f| %>
	<%= f.label :name,"Teacher name"%><br>
	<%= f.text_area :name %><br>

	<%= f.label :description,"Teacher Description"%><br>
	<%= f.text_area :description %><br>

	<%= f.label :country,"Country"%><br>
	<%= f.text_area :country %><br>

	<%= f.label :age,"Age"%><br>
	<%= f.text_area :age %><br>

	<%= f.submit 'Create' %>
<%end %>
<!-- </div> -->

<%= link_to "Back to Index" , bookings_path%>